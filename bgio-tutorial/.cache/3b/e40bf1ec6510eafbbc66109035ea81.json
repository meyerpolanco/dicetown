ion IsDraw(cells) {\n    return cells.filter(c => c === null).length === 0;\n  }\n\nexport const TicTacToe = {\n    setup: () => ({ cells: Array(9).fill(null) }),\n\n    turn: {\n        minMoves: 1,\n        maxMoves: 1,\n    },\n  \n    moves: {\n      clickCell: ({ G, playerID }, id) => {\n        if (G.cells[id] !== null) {\n            return INVALID_MOVE;\n          }\n          G.cells[id] = playerID;\n      },\n    },\n\n    endIf: ({ G, ctx }) => {\n        if (IsVictory(G.cells)) {\n            return { winner: ctx.currentPlayer };\n          }\n          if (IsDraw(G.cells)) {\n            return { draw: true };\n    }\n  },\n}"},"lineCount":null}},"error":null,"hash":"cb4f757c72ccf81b354225a976c06b5c","cacheData":{"env":{}}}